{
  "name": "PWA-Jest-Tailwind",
  "short_name": "Next PWA",
  "description": "POC using PWA with Jest tests",
  "theme_color": "#ffffff",
  "background_color": "#ffffff",
  "display": "standalone",
  "orientation": "any",
  "dir": "ltr",
  "prefer_related_applications": false,
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "display_override": ["window-controls-overlay"],
  "id": "/",
  "scope": "/",
  "lang": "en",
  "start_url": "/",
